<body>
    <div class="event-add-container">
        <form class="event-add-form" method="POST" action="/test/upload" enctype="multipart/form-data">
            <div class="event-add-sector-container">
                <div class="event-add-sector">
                    <div class="event-add-sector-main-title">Main Title 1</div>
                    <div class="event-add-sector-content">
                        <div class="event-add-sector-sub-content-container event-add-required event-add-addable event-add-searchable">
                            <div class="event-add-sector-sub-title">Sub Title 1-1</div>
                            <div id="get-category" class="event-add-sector-sub-content-container">
                                <div class="event-add-sector-sub-content event-add-addable-content event-add-searchable-content">
                                    <div class="event-add-sector-sub-content-input-container">
                                        <div class="fa fa-close"></div>
                                        <input id="subcontenttext1" class="event-add-sector-sub-content-input event-add-input-text-input-text" type="text" name="subcontenttext1">
                                        <input id="subcontenthidden1" class="event-add-sector-sub-content-input event-add-input-text-input-hidden" type="hidden" name="subcontenthidden1">
                                    </div>
                                    <div class="event-add-sector-sub-content-search-box">
                                        <ul class="event-add-sector-sub-content-search-ul">
                                            <li class="event-add-sector-sub-content-search-li">
                                                <div class="event-add-sector-sub-content-search-content">
                                                    <div>This is option 1</div>
                                                </div>
                                            </li>
                                            <li class="event-add-sector-sub-content-search-li">
                                                <div class="event-add-sector-sub-content-search-content">
                                                    <div>This is option 2</div>
                                                </div>
                                            </li>
                                            <li class="event-add-sector-sub-content-search-li">
                                                <div class="event-add-sector-sub-content-search-content">
                                                    <div>This is option 3</div>
                                                </div>
                                            </li>
                                            <li class="event-add-sector-sub-content-search-li">
                                                <div class="event-add-sector-sub-content-search-content">
                                                    <div>This is option 4</div>
                                                </div>
                                            </li>
                                            <li class="event-add-sector-sub-content-search-li">
                                                <div class="event-add-sector-sub-content-search-content">
                                                    <div>This is option 5</div>
                                                </div>
                                            </li>
                                            <li class="event-add-sector-sub-content-search-li">
                                                <div class="event-add-sector-sub-content-search-content">
                                                    <div>This is option 6</div>
                                                </div>
                                            </li>
                                        </ul>
                                    </div>
                                    <div class="alert-msg-container">
                                        <div class="alert-msg alert-msg-error alert-msg-error-type-empty">Event title must not be empty or must be selected</div>
                                        <div class="alert-msg alert-msg-error alert-msg-error-type-connection">Event title must not be empty or must be selected</div>
                                    </div>
                                </div>
                            </div>
                            <div class="event-add-sector-sub-content-button">add button</div>
                        </div>
                    </div>
                </div>
                <div class="event-add-sector">
                        <div class="event-add-sector-main-title">Main Title 2</div>
                        <div class="event-add-sector-content">
                            <div class="event-add-sector-sub-content-container event-add-required event-add-addable">
                                <div class="event-add-sector-sub-title">Sub Title 2-1</div>
                                <div class="event-add-sector-sub-content-container">
                                    <div class="event-add-sector-sub-content">
                                        <div class="event-add-sector-sub-content-input-container">
                                            <input class="event-add-sector-sub-content-input event-add-input-text" type="text" name="texttpyemultipleinput1">
